target 'Firestripe' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Firestripe
  pod 'Stripe'
	pod 'Firebase/Auth'
	pod 'Firebase/Firestore'
	pod 'FirebaseFirestoreSwift'

end
